            "description": "Data for the solver in JSON format",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Model"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation"
          },
        }
      }
    }

  },
"definitions": {
    "Model": {
        "type": "object",
        "properties": {
            "language": {
                "type": "string",
                "example": "Python"
            },
            "framework": {
                "type": "string",
                "example": "Flask"
            },
            "website": {
                "type": "string",
                "example": "Scotch"
            },
            "version_info": {
                "type": "object",
                "properties": {
                    "python": {
                        "type": "string",
                        "example": "3.9.0"
                    },
                    "flask": {
                        "type": "string",
                        "example": "1.1.2"
                    }
                }
            },
            "examples": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "example": "['Rasmus', 'Ib']"
            },
            "boolean_test": {
                "type": "boolean",
